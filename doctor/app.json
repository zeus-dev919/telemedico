{
  "expo": {
    "name": "Medipocket World",
    "slug": "HealthcareApp",
    "version": "1.0.1",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "buildNumber": "1.0.2",
      "infoPlist": {
        "NSCameraUsageDescription": "This app uses the camera to use video call.",
        "NSMicrophoneUsageDescription": "This app uses the microphone to use video call."
      },
      "usesIcloudStorage": true,
      "bundleIdentifier": "com.rami404.doctor"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      },
      "versionCode": 3,
      "permissions": [
        "CAMERA",
        "RECORD_AUDIO"
      ],
      "package": "com.rami404.doctor"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "description": "",
    "plugins": [
    [
      "@stripe/stripe-react-native",
      {
        "merchantIdentifier": "",
        "enableGooglePay": true
      }
    ]
    ],
    "hooks": {
      "postPublish": [
        {
          "file": "sentry-expo/upload-sourcemaps",
          "config": {
            "organization": "ramirez404",
            "project": "ramirez404",
            "authToken": "f9cec2e31ac84522a5e1dffa54c330df69942672800e46f884b9dcf3914fa3a7"
          }
        }
      ]
    }
  }
}
